{
	"manifest_version": 3,
	"name": "Images Reloader",
	"version": "1.2",
	"description": "This extension allows you to reload images which failed to load just in one click",
	"permissions": ["activeTab", "scripting"],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"action": {
		"default_icon": {
			"128": "img/icon_128.png",
			"19": "img/icon_19.png",
			"256": "img/icon_256.png",
			"38": "img/icon_38.png",
			"512": "img/icon_512.png"
		},
		"default_title": "Click to reload images"
	},
	"background": {
		"service_worker": "bg_script.js"
	},
	"content_scripts": [
		{
			"css": ["cs_styles.css"],
			"js": ["jquery.js", "cs_script.js", "sc_notificator.js"],
			"matches": ["<all_urls>"],
			"run_at": "document_start" // Run as soon as the document starts loading
    }
  ],
	"web_accessible_resources": [
		{
			"resources": [
        "img/icon_128.png",
        "img/icon_19.png",
        "img/icon_256.png",
        "img/icon_38.png",
        "img/icon_512.png",
        "img/loader.gif",
        "img/done.gif",
        "img/warning.gif"
      ],
			"matches": ["<all_urls>"]
    }
  ],
	"key": ""
}
